{
  "name": "@dfinity/service-worker",
  "description": "Service worker to check certification when loading assets from the Internet Computer.",
  "version": "1.2.0",
  "license": "Apache-2.0",
  "files": [
    "dist-dev",
    "dist-prod",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dfinity/ic.git",
    "directory": "typescript/service-worker"
  },
  "dependencies": {
    "@dfinity/agent": "^0.11.1",
    "@dfinity/candid": "^0.11.1",
    "@dfinity/principal": "^0.11.1",
    "base64-arraybuffer": "^1.0.2",
    "pako": "^2.0.3"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@types/pako": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "assert": "^2.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-jsdoc": "^38.0.6",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "jest-cli": "^27.5.1",
    "jest-environment-node": "^27.5.1",
    "jest-expect-message": "^1.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.0.0",
    "prettier": "^2.6.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "text-encoding": "^0.7.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "clean": "rm -fr dist dist-dev dist-prod coverage",
    "build": "rm -fr dist dist-prod && NODE_OPTIONS=--openssl-legacy-provider webpack && mv dist dist-prod",
    "build-dev": "rm -fr dist dist-dev && NODE_OPTIONS=--openssl-legacy-provider FORCE_FETCH_ROOT_KEY=1 webpack --mode development && mv dist dist-dev",
    "start": "npm run build-dev && http-server dist-dev --proxy https://ic0.app/",
    "start-testnet": "npm run build-dev && http-server dist-dev --proxy https://identity.dfinity.network/",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "test:coverage": "jest --verbose --collectCoverage",
    "test": "jest",
    "prepare": "npm run build && npm run build-dev",
    "prettier:check": "npx prettier --check src",
    "prettier:format": "npx prettier --write src",
    "format:check": "npm run prettier:check",
    "format:fix": "npm run prettier:format",
    "format": "npm run prettier:format"
  },
  "engines": {
    "node": "^17",
    "npm": "^8.5"
  },
  "keywords": [
    "internet computer",
    "internet-computer",
    "ic",
    "dfinity",
    "dfx",
    "canister",
    "candid",
    "motoko",
    "javascript",
    "typescript",
    "blockchain",
    "crypto",
    "dapp",
    "service worker",
    "service-worker"
  ]
}
